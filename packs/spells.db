{"_id":"omZGjnaoQPIAeivS","name":"Shield","type":"spell","img":"systems/dnd5e/icons/spells/protect-magenta-1.jpg","data":{"description":{"value":"\u003cp\u003eAn invisible barrier of magical force appears and protects you. Until the start of your next turn, you have a +5 bonus to AC, including against the triggering attack, and you take no damage from magic missile.\u003c/p\u003e","chat":"","unidentified":""},"source":"PHB pg. 275","activation":{"type":"reaction","cost":1,"condition":"Which you take when you are hit by an attack or targeted by the magic missile spell"},"duration":{"value":1,"units":"round"},"target":{"units":"","type":"self"},"range":{"units":"self"},"uses":{"value":0,"max":"0","per":""},"consume":{"type":"","target":""},"ability":"","actionType":"util","attackBonus":0,"chatFlavor":"","critical":{},"damage":{"parts":[],"versatile":"","value":""},"formula":"","save":{"ability":"","scaling":"spell","value":""},"level":1,"school":"abj","components":{"value":"","vocal":true,"somatic":true,"material":false,"ritual":false,"concentration":false},"materials":{"value":"","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"prepared","prepared":false},"scaling":{"mode":"none","formula":""}},"effects":[{"_id":"hzQdCAC3gFDfLREf","flags":{},"changes":[{"key":"data.attributes.ac.bonus","mode":2,"value":"+5","priority":"20"}],"disabled":false,"duration":{},"icon":"systems/dnd5e/icons/spells/protect-magenta-1.jpg","label":"Shield","transfer":false,"selectedKey":"data.attributes.ac.bonus"}],"sort":0,"permission":{"default":0,"gSvPH9u5wzkE1oU1":3,"5vlmE3uiS7OIQTEf":3},"flags":{"_sheetTab":"description","dynamiceffects":{"equipActive":true,"alwaysActive":false,"effects":[{"modSpecKey":"data.attributes.ac.value","value":"5","mode":"+","targetSpecific":false,"id":1,"itemId":"sEwmlXP3wXnd41oR","active":true,"_targets":[]}]},"midi-qol":{"onUseMacroName":"","effectActivation":false,"forceCEOff":false},"core":{"sourceId":"Compendium.Dynamic-Effects-SRD.DAE SRD Spell