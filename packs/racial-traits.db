,\n\t\t\tdisabled: false,\n\t\t\tduration: {\n\t\t\t\trounds: 1,\n\t\t\t\tstartTime: null\n\t\t\t},\n\t\t\t\n\t\t};\n\n\t\tfor (let ttoken of enemies) {\n\t\t\tawait MidiQOL.socket().executeAsGM(\"createEffects\", { actorUuid: ttoken.actor.uuid, effects: [effectData] });\n\t\t}\n\n\t}\n\t\n\t\n} catch (err) {\n    console.error(`${optionName} : ${version}`, err);\n}","folder":null,"sort":0,"permission":{"default":0},"flags":{}}},"autoanimations":{"version":4,"killAnim":false,"animLevel":false,"options":{"ammo":false,"staticType":"source","menuType":"spell","variant":"intro","enableCustom":false,"repeat":2,"delay":null,"scale":null,"opacity":null,"unbindAlpha":false,"unbindVisibility":false,"persistent":false,"scaleX":null,"scaleY":null,"removeTemplate":false,"auraRadius":null,"ignoreTarget":false},"override":true,"sourceToken":{"enable":false},"targetToken":{"enable":false},"levels3d":{"type":""},"macro":{"enable":false},"animType":"static","animation":"bless","color":"purple","audio":{"a01":{"enable":false}},"preview":false,"explosions":{"enable":false}},"scene-packer":{"sourceId":"Compendium.fvtt-trazzm-homebrew-5e.homebrew-racial-traits.tVeOzFhytdH6ZtL3"}},"system":{"description":{"value":"<p>As a bonus action, you let out a cry at your enemies within 10 feet of you. Until the start of your next turn, you and your allies have advantage on attack rolls against any of those enemies who could hear you. You can use this trait a number of times equal to your proficiency bonus, and you regain all expended uses when you finish a long rest.</p>\n\n","chat":"","unidentified":""},"source":"Mordenkainen Presents: Monsters of the Multiverse pg 25","activation":{"type":"bonus","cost":1,"condition":""},"duration":{"value":1,"units":"round"},"target":{"value":null,"width":null,"units":"","type":"self"},"range":{"value":null,"long":null,"units":"self"},"uses":{"value":2,"max":"@prof","per":"lr","recovery":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionTy