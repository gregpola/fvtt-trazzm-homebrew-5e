paz","img":"icons/svg/d20-black.svg","weight":1,"range":[9,9],"drawn":false,"_id":"9vaf4ftqlxeky3k9"},{"flags":{},"type":0,"text":"<b>[[+ 10]] ([[/r + 10]])</b>10\n\n<b>Damage Type</b>Thunder\n\n<b>Gem</b>Spinel","img":"icons/svg/d20-black.svg","weight":1,"range":[10,10],"drawn":false,"_id":"ppuz8gg5moony31p"}],"formula":"d10","replacement":true,"displayRoll":true,"folder":null,"sort":0,"permission":{"default":0},"flags":{}}
